from collections import Counter

if __name__ == '__main__':
    s = input().strip()

    # Count character occurrences
    counts = Counter(s)

    # Sort by (-frequency, character)
    sorted_chars = sorted(counts.items(), key=lambda x: (-x[1], x[0]))

    # Print top 3
    for char, freq in sorted_chars[:3]:
        print(char, freq)
