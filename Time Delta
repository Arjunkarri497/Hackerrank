from datetime import datetime

# Number of test cases
t = int(input().strip())

for _ in range(t):
    # Read the two timestamps
    ft1 = input().strip()
    t2 = input().strip()

    # Parse timestamps with timezone
    time1 = datetime.strptime(t1, "%a %d %b %Y %H:%M:%S %z")
    time2 = datetime.strptime(t2, "%a %d %b %Y %H:%M:%S %z")

    # Compute absolute difference in seconds
    diff = abs(int((time1 - time2).total_seconds()))

    # Output result
    print(diff)

